{
  "manifest_version": 3,
  "name": "Save to Downloads",
  "version": "0.2.0",
  "description": "An easy way to save images to the Downloads folder.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "downloads",
    "contextMenus"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }
}