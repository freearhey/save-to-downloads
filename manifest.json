{
  "manifest_version": 2,
  "name": "Save to Downloads",
  "version": "0.1.0",
  "description": "Adds a link to the context menu for direct downloading of images to the Downloads folder.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "downloads",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}